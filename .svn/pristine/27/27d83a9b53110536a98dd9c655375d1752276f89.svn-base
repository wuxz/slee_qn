package com.channelsoft.slee.usmlreasoning;

import com.channelsoft.slee.util.Constant;

public class USMLFunction
{
	static int transErrToInt(String eventType)
	{
		if (Constant.DestBusy.equals(eventType))
		{
			return Constant.EVENT_DestBusy;
		}

		if (Constant.NoFaxDevice.equals(eventType))
		{
			return Constant.EVENT_NoFaxDevice;
		}

		if (Constant.NoCall.equals(eventType))
		{
			return Constant.EVENT_NoCall;
		}

		if (Constant.InvalidFile.equals(eventType))
		{
			return Constant.EVENT_InvalidFile;
		}

		if (Constant.DiskFull.equals(eventType))
		{
			return Constant.EVENT_DiskFull;
		}

		if (Constant.NoAnswer.equals(eventType))
		{
			return Constant.EVENT_NoAnswer;
		}

		if (Constant.TimeOut.equals(eventType))
		{
			return Constant.EVENT_TimeOut;
		}

		if (Constant.InvalidTelNum.equals(eventType))
		{
			return Constant.EVENT_InvalidTelNum;
		}

		if (Constant.CustomHangup.equals(eventType))
		{
			return Constant.EVENT_CustomHangup;
		}

		if (Constant.ToAgent.equals(eventType))
		{
			return Constant.EVENT_ToAgent;
		}

		if (Constant.GeneralError.equals(eventType))
		{
			return Constant.EVENT_GeneralError;
		}

		if (Constant.No_Error.equals(eventType))
		{
			return Constant.EVENT_No_Error;
		}

		if (Constant.ScriptRunError.equals(eventType))
		{
			return Constant.EVENT_ScriptRunError;
		}

		if (Constant.CantFindUSMLFile.equals(eventType))
		{
			return Constant.EVENT_CantFindUSMLFile;
		}

		if (Constant.InvalidUSMLFile.equals(eventType))
		{
			return Constant.EVENT_InvalidUSMLFile;
		}

		if (Constant.UnsupportProc.equals(eventType))
		{
			return Constant.EVENT_UnsupportProc;
		}

		if (Constant.InvalidParam.equals(eventType))
		{
			return Constant.EVENT_IVALID_PARAM;
		}

		if (Constant.InvalidTerminal.equals(eventType))
		{
			return Constant.EVENT_IVALID_Teminal;
		}

		if (Constant.No_Authorization.equals(eventType))
		{
			return Constant.EVENT_No_Authorization;
		}

		if (Constant.InvalidAuthorization.equals(eventType))
		{
			return Constant.EVENT_InvalidAuthorization;
		}

		if (Constant.Over_ValidDateTime.equals(eventType))
		{
			return Constant.EVENT_Over_ValidDateTime;
		}

		if (Constant.InvalidNodeCode.equals(eventType))
		{
			return Constant.EVENT_InvalidNodeCode;
		}

		if (Constant.StackOverflow.equals(eventType))
		{
			return Constant.EVENT_StackOverflow;
		}
		
		if(Constant.UnsupportedMedia.equals(eventType))
		{
			return Constant.EVENT_UnsupportedMedia;
		}
		
		if(Constant.MediaUnmatch.equals(eventType))
		{
			return Constant.EVENT_MediaUnmatch;
		}
		
		return 0;
	}

	static int transPronLangToInt(String value)
	{

		if (Constant.MaleMandarin.equals(value))
		{
			return Constant.INT_MaleMandarin;
		}

		if (Constant.FemaleMandarin.equals(value))
		{
			return Constant.INT_FemaleMandarin;
		}

		if (Constant.MaleCantonese.equals(value))
		{
			return Constant.INT_MaleCantonese;
		}

		if (Constant.FemaleCantonese.equals(value))
		{
			return Constant.INT_FemaleCantonese;
		}

		if (Constant.MaleEnglish.equals(value))
		{
			return Constant.INT_MaleEnglish;
		}

		if (Constant.FemaleEnglish.equals(value))
		{
			return Constant.INT_FemaleEnglish;
		}

		return Constant.INT_FemaleMandarin;

	}
}
