	public Variant clear(Variant[] args) throws Exception
	{
		return new Variant("", cc);
	}

	public Variant description(Variant[] args) throws Exception
	{
		return new Variant("", cc);
	}

	/* (non-Javadoc)
	 * @see com.channelsoft.slee.usmlreasoning.InternalObject#getDefaultPropertyName()
	 */
	@Override
	public String getDefaultPropertyName()
	{
		return null;
	}

	@Override
	public Variant getProperty(String propertyName, Variant[] args)
			throws Exception
	{
		return null;
	}

	public Variant helpcontext(Variant[] args) throws Exception
	{
		return new Variant("", cc);
	}

	public Variant helpfile(Variant[] args) throws Exception
	{
		return new Variant("", cc);
	}

	/* (non-Javadoc)
	 * @see com.channelsoft.reusable.comobj.IDispatch#invoke(java.lang.String, com.channelsoft.reusable.util.Variant[])
	 */
	@Override
	public Variant invoke(String method, Variant[] args) throws Exception
	{
		return getProperty(method, args);
	}

	public Variant number(Variant[] args) throws Exception
	{
		return new Variant("", cc);
	}

	public Variant raise(Variant[] args) throws Exception
	{
		return new Variant("", cc);
	}

